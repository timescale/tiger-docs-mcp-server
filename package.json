{
  "name": "@tigerdata/tiger-docs-mcp-server",
  "version": "0.1.0",
  "description": "MCP server to expose an API around our embedded docs database",
  "license": "UNLICENSED",
  "author": "TigerData",
  "homepage": "https://tigerdata.com",
  "type": "module",
  "bin": {
    "tiger-docs-mcp-server": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && rm -f dist/prompts/md/* && shx cp -r src/prompts/md dist/prompts/ && shx chmod +x dist/*.js",
    "prepare": "npm run build",
    "watch": "tsx watch src/index.ts stdio",
    "watch:http": "tsx watch src/index.ts http",
    "start": "node dist/index.js stdio",
    "start:http": "node dist/index.js http",
    "inspector": "npx @modelcontextprotocol/inspector",
    "migrate": "migrate"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.16",
    "@modelcontextprotocol/sdk": "^1.15.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.203.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.203.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.203.0",
    "@opentelemetry/instrumentation-http": "^0.203.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-logs": "^0.203.0",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "ai": "^5.0.17",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "gray-matter": "^4.0.3",
    "migrate": "^2.1.0",
    "pg": "^8.16.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^22.16.4",
    "@types/pg": "^8.15.5",
    "prettier": "^3.6.2",
    "shx": "^0.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
